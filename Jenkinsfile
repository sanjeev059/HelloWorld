pipeline {
  agent any
  stages {
    stage('Build Demo') {
      steps {
        echo 'In Production'
      }
    }
    stage('Promote Demo') {
      steps {
        echo 'Build Prod'
      }
    }
    stage('Deploy Demo') {
      steps {
        echo 'Deployed'
      }
    }
    stage('Test Demo') {
      steps {
        echo 'tested'
      }
    }
    stage('Artifacts') {
      steps {
        echo 'stored artifacts'
      }
    }
  }
}