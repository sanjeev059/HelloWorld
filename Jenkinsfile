pipeline {
  agent any
  stages {
    stage('Build Learn') {
      steps {
        echo 'In Production'
      }
    }
    stage('Promote Learn') {
      steps {
        echo 'Build Prod'
      }
    }
    stage('Deploy Learn') {
      steps {
        echo 'Deployed'
      }
    }
    stage('Test Learn') {
      steps {
        echo 'tested'
      }
    }
    stage('Artifacts') {
      steps {
        echo 'stored artifacts'
      }
    }
  }
}