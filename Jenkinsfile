pipeline {
  agent any
  stages {
    stage('Production') {
      steps {
        echo 'In Production'
      }
    }
    stage('Build') {
      steps {
        echo 'Building'
      }
    }
    stage('Deploy ') {
      steps {
        echo 'Deployed'
      }
    }
    stage('Test') {
      steps {
        echo 'tested'
      }
    }
    stage('Artifacts') {
      steps {
        echo 'stored artifacts'
      }
    }
  }
}